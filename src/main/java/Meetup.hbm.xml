<?xml version='1.0' encoding='UTF-8'?>   
<!DOCTYPE hibernate-mapping PUBLIC    
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.squadleader.meetups.meetup" table="meetups">
		
		<id name="EVENT_ID" column="EVENT_ID" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="GROUP_URL" column="GROUP_URL" type="string"></property>
		<property name="GROUP_NAME" column="GROUP_NAME" type="string"></property>
		<property name="EVENT_NAME" column="EVENT_NAME" type="string"></property>
		<property name="EVENT_DATE" column="EVENT_DATE" type="string"></property>
		<property name="EVENT_VENUE_NAME" column="EVENT_VENUE_NAME" type="string"></property>
		<property name="EVENT_STREET" column="EVENT_STREET" type="string"></property>
		<property name="EVENT_CITY" column="EVENT_CITY" type="string"></property>
		<property name="EVENT_STATE" column="EVENT_STATE" type="string"></property>
		<property name="EVENT_ZIP" column="EVENT_ZIP" type="string"></property>
		<property name="EVENT_LATITUDE" column="EVENT_LATITUDE" type="string"></property>
		<property name="EVENT_LONGITUDE" column="EVENT_LONGITUDE" type="string"></property>
		<property name="EVENT_LINK" column="EVENT_LINK" type="string"></property>
		
			
	</class>
</hibernate-mapping>