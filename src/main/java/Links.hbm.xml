<?xml version='1.0' encoding='UTF-8'?>   
<!DOCTYPE hibernate-mapping PUBLIC    
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.squadleader.links.Links" table="Links">

		<id name="Id" column="Id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="Category" column="Category" type="string"></property>
		<property name="Title" column="Title" type="string"></property>
		<property name="URL" column="URL" type="string"></property>
			
	</class>
</hibernate-mapping>