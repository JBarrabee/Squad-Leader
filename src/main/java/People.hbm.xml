<?xml version='1.0' encoding='UTF-8'?>   
<!DOCTYPE hibernate-mapping PUBLIC    
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.squadleader.people.Person" table="userAccounts">

		<id name="ID" column="ID" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="FIRST_NAME" column="FIRST_NAME" type="string"></property>
		<property name="LAST_NAME" column="LAST_NAME" type="string"></property>
		<property name="EMAIL" column="EMAIL" type="string"></property>
		<property name="PASS" column="PASS" type="string"></property>
		<property name="STREET_ADDRESS" column="STREET_ADDRESS" type="string"></property>
		<property name="CITY" column="CITY" type="string"></property>
		<property name="STATE" column="STATE" type="string"></property>
		<property name="LATITUDE" column="LATITUDE" type="string"></property>
		<property name="LONGITUDE" column="LONGITUDE" type="string"></property>
		<property name="DOB" column="DOB" type="date"></property>
		<property name="ACCESS_LEVEL" column="ACCESS_LEVEL" type="string"></property>

		
			
	</class>
</hibernate-mapping>